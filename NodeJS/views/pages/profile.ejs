<!doctype html>
<html>
<head>
    <title>Profile</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
        body        {word-wrap:break-word; }
    </style>
</head>
<body>
<div class="container">

    <div class="page-header text-left">
        <h1><span class="fa fa-anchor"></span> Profile Page</h1>
        
    </div>
    <div class="text-right">
        <a href="/logout" class="btn btn-default btn-default">Logout</a>
    </div>

    <div class="text-left">
        <a href="/form" class="btn btn-default btn-default">Form</a>
    </div>

    <div class="row">

        <!-- GOOGLE INFORMATION -->
        <div class="col-sm-6">
            <div class="well">
                <h3 class="text-danger"><span class="fa fa-google-plus"></span> Google</h3>

                    <p>
                        <strong>Email ID</strong>: <%= email %> <br>
                        <strong>Name</strong>: <%= name %> <br>
                        <strong>Profile Picture</strong>: <img src="<%=pic%>" width="100" height="100" alt="">
                    </p>

            </div>
        </div>

    </div>

</div>
</body>
</html>